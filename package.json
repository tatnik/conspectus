  "scripts": {
    "generate-search-index": "pnpm exec ts-node scripts/generateSearchIndex.ts",
    "predeploy": "pnpm exec ts-node scripts/generateSearchIndex.ts && pnpm run build",
    "deploy": "gh-pages -d build",
    "start": "react-app-rewired start",
    "clean": "rimraf build",
    "build": "pnpm run clean && react-app-rewired build && cp build/index.html build/404.html",
    "test": "react-app-rewired test ",
    "test:ci": "cross-env CI=true react-app-rewired test ",
    "lint:js": "eslint --ext .js,.mjs,.jsx,.ts,.tsx .",
    "lint:styles": "stylelint '{styles,src}/**/*.{scss,css}'",
    "lint:prettier": "prettier --check \"src/**/*.{ts,tsx,js,mjs,jsx,json,css,scss,md}\"",
    "lint-fix:prettier": "prettier --write \"src/**/*.{ts,tsx,js,mjs,jsx,json,css,scss,md}\"",
    "lint": "pnpm run lint:js && pnpm run lint:styles && pnpm run lint:prettier",
    "lint-fix:js": "eslint  --ext .js,.mjs,.jsx,.ts,.tsx . --fix",
    "lint-fix:styles": "stylelint '{styles,src}/**/*.scss' --fix",
    "test:create": "node .testCreator/scripts/testCreator.mjs",
    "docker:up": "docker-compose -f docker/docker-compose.yaml up",
    "docker:down": "docker-compose -f docker/docker-compose.yaml down",
    "docker:build": "docker-compose -f docker/docker-compose.yaml build",
    "docker:logs": "docker-compose -f docker/docker-compose.yaml logs -f",
    "docker:ps": "docker-compose -f docker/docker-compose.yaml ps"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://tatnik.github.io/conspectus"
}
